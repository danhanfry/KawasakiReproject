{
	"compilerOptions": {
		"target": "es2015",
		"module": "es2015",
		"moduleResolution": "node",
		"isolatedModules": false,
		"jsx": "react",
		"rootDir": "src",
		"allowSyntheticDefaultImports": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"declaration": false,
		"noImplicitAny": false,
		"removeComments": true,
		"noLib": false,
		"preserveConstEnums": true,
		"suppressImplicitAnyIndexErrors": true,
        "allowJs": true,
        "sourceMap": true
	},
	"filesGlob": [
		"**/*.ts",
		"**/*.tsx",
		"!node_modules/**"
	],
	"files": [
		"src/sections/commercial/components/commercialscroller.tsx"
	],
	"exclude": []
}