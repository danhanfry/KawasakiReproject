{
	"compilerOptions": {
		"target": "es5",
		"module": "amd",
		"moduleResolution": "node",
		"isolatedModules": false,
		"jsx": "react",
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"declaration": false,
		"noImplicitAny": false,
		"removeComments": true,
		"noLib": false,
		"preserveConstEnums": true,
		"suppressImplicitAnyIndexErrors": true
	},
	"filesGlob": [
		"**/*.ts",
		"**/*.tsx",
		"!node_modules/**"
	],
	"files": [
		"KawasakiNinja650/js/sections/commercial/commercial.tsx",
		"KawasakiNinja650/js/sections/commercial/components/commercialvideo.tsx",
		"KawasakiNinja650/js/sections/commercial/components/commercialcontent.tsx",
		"KawasakiNinja650/js/sections/commercial/components/commercialscroller.tsx",
		"KawasakiNinja650/js/sections/vtr/vtr.tsx",
		"KawasakiNinja650/js/sections/vtr/components/vtrvideo.tsx",
		"KawasakiNinja650/js/sections/vtr/components/vtrcontent.tsx",
		"KawasakiNinja650/js/sections/vtr/components/vtrmodal.tsx",
		"KawasakiNinja650/js/sections/researchtools/components/researchtoolsheader.tsx",
		"KawasakiNinja650/js/sections/researchtools/components/researchtoolslinks.tsx",
		"KawasakiNinja650/js/sections/researchtools/researchtools.tsx"
	],
	"exclude": [ ]
}
